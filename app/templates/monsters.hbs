<h1>Monsters</h1>

<div class="row">
  <div class="col-sm-4 col-xs-6">
    <ul class="monsters-index-list">
      {{#each model as |monster|}}
        <li class="monster list-unstyled">
          <span class="monster-image">
            <img src={{monster.imageUrl}} />
          </span>
          <span class="monster-info">
            lvl {{monster.level}} {{monster.name}}
          </span>
        </li>
      {{/each}}
    </ul>
  </div>
</div>
